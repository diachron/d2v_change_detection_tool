query=SELECT ?var1 ?var2 WHERE { \n\
GRAPH <v2> {  \n\
?var1 qb:component ?cs. \n\
?cs qb:measure ?var2. \n\
} \n\
FILTER NOT EXISTS { GRAPH <v1> { \n\
?var1 qb:component ?cs. \n\
?cs qb:measure ?var2. \n\
}\n\
}\n\
}\n\

type=a <http://www.diachron-fp7.eu/changes/Attach_Measure_To_FT>
var1=<http://www.diachron-fp7.eu/changes/attMeasToFT_p1>
var2=<http://www.diachron-fp7.eu/changes/attMeasToFT_p2>

type_var1=URI
type_var2=URI