query=SELECT ?var1 ?var2 ?var3 WHERE { \n\
GRAPH <v1> { \n\
?var1 ?var2 ?var3. \n\
filter (?var2 = rdfs:label).\n\
} \n\
FILTER NOT EXISTS { GRAPH <v2> { \n\
?var1 ?var2 ?var3 \n\
}\n\
}\n\
}

type=a <http://www.diachron-fp7.eu/changes/Delete_Label>
var1=<http://www.diachron-fp7.eu/changes/delLab_p1>
var1=<http://www.diachron-fp7.eu/changes/delLab_p2>
var1=<http://www.diachron-fp7.eu/changes/delLab_p3>

type_var1=URI
type_var2=LITERAL
type_var3=LITERAL