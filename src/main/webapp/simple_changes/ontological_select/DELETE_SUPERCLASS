query=SELECT ?var1 ?var2 WHERE { \n\
GRAPH <v1> { ?var1 rdfs:subClassOf ?var2 } \n\
FILTER NOT EXISTS { GRAPH <v2> { ?var1 rdfs:subClassOf ?var2 } } \n\
}

type=a <http://www.diachron-fp7.eu/changes/Delete_Superclass>
var1=<http://www.diachron-fp7.eu/changes/dsc_p1>
var2=<http://www.diachron-fp7.eu/changes/dsc_p2>

type_var1=URI
type_var2=URI