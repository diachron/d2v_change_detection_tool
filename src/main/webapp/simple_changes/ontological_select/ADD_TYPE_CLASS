query=SELECT ?var1 WHERE { \n\
GRAPH <v2> { \n\
{ ?var1 rdf:type rdf:Class. } UNION { ?var1 rdf:type owl:Class. } \n\
}\n\
FILTER NOT EXISTS { GRAPH <v1> {\n\
{ ?var1 rdf:type rdf:Class. } UNION { ?var1 rdf:type owl:Class. } \n\
}\n\
}\n\
}\n\

