query=SELECT ?var1 ?var2 WHERE { \n\
GRAPH <v2> { ?var1 rdfs:range ?var2 } \n\
FILTER NOT EXISTS { GRAPH <v1> { ?var1 rdfs:range ?var2 } } \n\
}

type=a <http://www.diachron-fp7.eu/changes/Add_Range>
var1=<http://www.diachron-fp7.eu/changes/ar_p1>
var2=<http://www.diachron-fp7.eu/changes/ar_p2>

type_var1=URI
type_var2=URI